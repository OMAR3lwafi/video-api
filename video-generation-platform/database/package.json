{
  "name": "video-generation-database",
  "version": "1.0.0",
  "description": "Database migrations and functions for Dynamic Video Content Generation Platform",
  "main": "index.js",
  "scripts": {
    "migrate:up": "supabase db push",
    "migrate:down": "supabase db reset",
    "migrate:status": "supabase migration list",
    "migrate:new": "supabase migration new",
    "seed": "supabase db seed",
    "reset": "supabase db reset",
    "start": "supabase start",
    "stop": "supabase stop",
    "status": "supabase status",
    "types:generate": "supabase gen types typescript --local > types/database.types.ts",
    "functions:deploy": "supabase functions deploy",
    "functions:serve": "supabase functions serve",
    "test": "jest",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --write \"**/*.{js,ts,sql}\""
  },
  "keywords": [
    "supabase",
    "postgresql",
    "database",
    "migrations",
    "video-generation"
  ],
  "author": "Video Generation Platform Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "supabase": "^1.123.4",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
