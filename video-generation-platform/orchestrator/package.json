{
  "name": "video-generation-orchestrator",
  "version": "1.0.0",
  "description": "Orchestrator service for Dynamic Video Content Generation Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t video-generation-orchestrator .",
    "docker:run": "docker run -p 9000:9000 video-generation-orchestrator"
  },
  "keywords": [
    "orchestrator",
    "workflow",
    "video-generation",
    "typescript"
  ],
  "author": "Video Generation Platform Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
