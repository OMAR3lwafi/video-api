{
  "name": "video-generation-backend",
  "version": "1.0.0",
  "description": "Backend API for Dynamic Video Content Generation Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:debug": "tsx watch --inspect src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:performance": "jest --testPathPattern=tests/performance",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=2",
    "test:load": "artillery run tests/performance/load-test.yml",
    "migrate:test": "NODE_ENV=test npm run migrate:up",
    "migrate:rollback-test": "NODE_ENV=test npm run migrate:down",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t video-generation-backend .",
    "docker:run": "docker run -p 3000:3000 video-generation-backend"
  },
  "keywords": [
    "video",
    "generation",
    "api",
    "express",
    "typescript",
    "ffmpeg"
  ],
  "author": "Video Generation Platform Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.454.0",
    "@aws-sdk/lib-storage": "^3.454.0",
    "@aws-sdk/s3-request-presigner": "^3.454.0",
    "@supabase/supabase-js": "^2.38.4",
    "@types/pg": "^8.15.5",
    "axios": "1.6.8",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "ffprobe-static": "^3.1.0",
    "file-type": "^21.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "ioredis": "5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "pidusage": "3.0.2",
    "prom-client": "^15.1.3",
    "sharp": "^0.33.0",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/dompurify": "^3.2.0",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.15.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "artillery": "^2.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.4",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
